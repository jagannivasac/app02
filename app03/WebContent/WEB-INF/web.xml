    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>FirstServlet</display-name>
    <servlet-name>FirstServlet</servlet-name>
    <servlet-class>com.durgasoft.servlet.FirstServlet</servlet-class>
    <init-param>
      <description></description>
      <param-name>a</param-name>
      <param-value>AAA</param-value>
    </init-param>
    <init-param>
      <description></description>
      <param-name>b</param-name>
      <param-value>BBB</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>FirstServlet</servlet-name>
    <url-pattern>/First</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SecoundServlet</display-name>
    <servlet-name>SecoundServlet</servlet-name>
    <servlet-class>com.durgasoft.servlet.SecoundServlet</servlet-class>
    <init-param>
      <description></description>
      <param-name>c</param-name>
      <param-value>CCC</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>SecoundServlet</servlet-name>
    <url-pattern>/SecoundServlet</url-pattern>
  </servlet-mapping>
</web-app>